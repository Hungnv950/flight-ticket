    <main class="main">
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="/admin/dashboard">Trang chủ</a>
            </li>
            <li class="breadcrumb-item">
                <a href="/admin/collaborator/index">Cộng tác viên</a>
            </li>
            <li class="breadcrumb-item active"><%= collaborator.fullName %></li>
        </ol>
        <div class="container-fluid">
            <div id="ui-view">
                <div>
                    <div class="fade-in">
                        <div class="row">
                            <div class="col-lg-3">
                                <div class="card">
                                    <div class="card-header">
                                        <p style="float: left;font-weight: bold;margin-top: 8px;margin-bottom: 0;">
                                            Tìm kiếm
                                        </p>
                                        <div class="card-header-actions">
                                            <a href="create" class="btn btn-block btn-outline-dark">
                                                <i class="nav-icon icon-plus"></i>
                                            </a>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div class="form-group">
                                                    <input class="form-control" id="name" type="text"
                                                           placeholder="Theo tên, mã CTV">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div class="form-group">
                                                    <input class="form-control" id="ccnumber" type="text"
                                                           placeholder="0000 0000 0000 0000">
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="form-group col-sm-4">
                                                <label for="ccmonth">Month</label>
                                                <select class="form-control" id="ccmonth">
                                                    <option>1</option>
                                                    <option>2</option>
                                                    <option>3</option>
                                                    <option>4</option>
                                                    <option>5</option>
                                                    <option>6</option>
                                                    <option>7</option>
                                                    <option>8</option>
                                                    <option>9</option>
                                                    <option>10</option>
                                                    <option>11</option>
                                                    <option>12</option>
                                                </select>
                                            </div>
                                            <div class="form-group col-sm-4">
                                                <label for="ccyear">Year</label>
                                                <select class="form-control" id="ccyear">
                                                    <option>2014</option>
                                                    <option>2015</option>
                                                    <option>2016</option>
                                                    <option>2017</option>
                                                    <option>2018</option>
                                                    <option>2019</option>
                                                    <option>2020</option>
                                                    <option>2021</option>
                                                    <option>2022</option>
                                                    <option>2023</option>
                                                    <option>2024</option>
                                                    <option>2025</option>
                                                </select>
                                            </div>
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label for="cvv">CVV/CVC</label>
                                                    <input class="form-control" id="cvv" type="text" placeholder="123">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-footer">
                                        <button class="btn btn-sm btn-primary pull-right" type="submit">
                                            <i class="nav-icon icon-check"></i>
                                            Tìm kiếm
                                        </button>
                                        <button class="btn btn-sm btn-danger pull-left" type="reset">
                                            <i class="nav-icon icon-close"></i>
                                            Khôi phục
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-9">
                                <div class="card">
                                    <div class="card-header">
                                        <p style="float: left;font-weight: bold;margin-top: 8px;margin-bottom: 0;">
                                            Cộng tác viên
                                        </p>
                                        <div class="card-header-actions">
                                            <select class="form-control" id="ccmonth">
                                                <option>Hôm nay</option>
                                                <option>Hôm qua</option>
                                                <option>7 ngày qua</option>
                                                <option>Tháng này</option>
                                                <option>Tháng trước</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="card-body" style="padding: 0;">
                                        <div class="content-list row row-padding-15 mb15" style="padding: 15px 20px;">
                                            <div class="col-md-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="form-label control-label">Mã CTV:</label>
                                                    <div class="form-wrap form-control-static">
                                                        <div class="input-group mb-3" id="product_coupon">
                                                            <input type="text" class="form-control"
                                                                   value="<%= collaborator.code %>" disabled="">
                                                            <div class="input-group-append">
                                                                <span class="input-group-text">
                                                                    <i class="nav-icon icon-docs"></i>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="form-label control-label">Họ và tên:</label>
                                                    <div class="form-wrap form-control-static">
                                                        <strong><%= collaborator.fullName %></strong>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="form-label control-label">
                                                        Số điện thoại:
                                                    </label>
                                                    <div class="form-wrap form-control-static payOrder">
                                                        <a href="javascript:void(0)"><%= collaborator.phone ? collaborator.phone : 'N\A' %></a>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="form-label control-label">Email:</label>
                                                    <div class="form-wrap form-control-static payOrder">
                                                        <%= collaborator.email ? collaborator.email : 'N\A' %>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-12 col-sm-12">
                                                <br>
                                            </div>
                                            <div class="col-md-12 col-sm-12" style="text-align: right;">
                                                <a href="/admin/collaborator/update/<%= collaborator.id %>"
                                                   class="btn btn-success btn-sm">
                                                    <i class="nav-icon icon-check"></i>
                                                    Cập nhật
                                                </a>
                                                <a href="/admin/product/update?id=4" class="btn btn-primary btn-sm">
                                                    <i class="nav-icon icon-key"></i>
                                                    Đổi mật khẩu
                                                </a>
                                                <a class="btn btn-danger btn-sm"
                                                   href="/admin/product/inactive?id=4">
                                                    <i class="nav-icon icon-lock"></i>
                                                    Ngừng hoạt động
                                                </a>
                                            </div>
                                        </div>
                                        <table class="table table-responsive-sm table-striped"
                                               style="margin-bottom: 0;">
                                            <thead>
                                            <tr>
                                                <th style="width: 40px;">ID</th>
                                                <th>Họ và tên</th>
                                                <th class="th-text-right">Số lượt đặt</th>
                                                <th class="th-text-right">Tổng tiền</th>
                                                <th class="th-text-right">Triết khấu</th>
                                            </tr>
                                            </thead>
                                            <tbody>
                                            <tr>
                                                <td></td>
                                                <td></td>
                                                <td class="th-text-right">0</td>
                                                <td class="th-text-right">0</td>
                                                <td class="th-text-right">0</td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </main>
</div>
